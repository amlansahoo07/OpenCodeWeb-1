// Write your code here
#include <stdio.h>
#include <string.h>
int main()
{
    int n,op,i;
    char seat[2];
    scanf("%d",&n);
    for(i=0;i<n;++i)
    {
     if(n%6==0)
     {
      strcpy(seat,"WS");
      if((n/6)%2==0)
        op=6((n/6)-2)+1;
      else op=6(n/6)+1;
     }
     else
        if(n%6==1)
        {
        strcpy(seat,"WS");
        if((n/6)%2==0)
            op=6((n/6)+2);
        else op=6(n/6);
        }
        else
            if(n%3==0)
            {
             strcpy(seat,"AS");
             if((n/3)%4==1)
                 op=3((n/3)+2)+1;
             else op=3((n/3)-2)+1;
            }
            else
                if(n%3==1)
                {
                 strcpy(seat,"AS");
                 if((n/3)%4==2)
                     op=3((n/3)-2);
                 else op=3((n/3)+2);
                }
                else
                    if(n%2==0)
                    {
                     strcpy(seat,"MS");
                     if((n/2)%2==0)
                         op=2((n/2)-2)+1;
                     else op=2((n/2)+4)+1;
                    }
                    else
                        if(n%2==1)
                        {
                         strcpy(seat,"MS");
                         if((n/2)%2==0)
                             op=2((n/2)+2);
                         else op=2((n/2)-4);
                        }
    printf("%d ",op);
    printf("%s\n",seat);
    }
    return 0;
}
